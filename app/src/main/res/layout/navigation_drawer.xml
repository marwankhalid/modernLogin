            <TextView
                android:id="@+id/seven_text"
                android:layout_width="0dp"
                android:layout_height="0dp"
                android:text="List Order"
                android:textSize="16dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintStart_toEndOf="@id/seven_image"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintHeight_percent=".6"
                app:layout_constraintWidth_percent=".8"
                >
            </TextView>

        </androidx.constraintlayout.widget.ConstraintLayout>
        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:id="@+id/eight"
            app:layout_constraintTop_toBottomOf="@id/seven"
            app:layout_constraintWidth_percent=".5"
            app:layout_constraintHeight_percent=".05"
            app:layout_constraintVertical_bias=".00"
            app:layout_constraintHorizontal_bias=".13"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            >

            <ImageView
                android:id="@+id/eight_image"
                android:layout_width="0dp"
                android:layout_height="0dp"
                android:src="@drawable/ic_launcher_background"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintHorizontal_bias=".00"
                app:layout_constraintHeight_percent="1"
                app:layout_constraintWidth_percent=".1"
                >
            </ImageView>

            <TextView
                android:id="@+id/eight_text"
                android:layout_width="0dp"
                android:layout_height="0dp"
                android:text="List Order"
                android:textSize="16dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintStart_toEndOf="@id/eight_image"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintHeight_percent=".6"
                app:layout_constraintWidth_percent=".8"
                >
            </TextView>

        </androidx.constraintlayout.widget.ConstraintLayout>
        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:id="@+id/nine"
            app:layout_constraintTop_toBottomOf="@id/eight"
            app:layout_constraintWidth_percent=".5"
            app:layout_constraintHeight_percent=".05"
            app:layout_constraintVertical_bias=".00"
            app:layout_constraintHorizontal_bias=".13"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            >

            <ImageView
                android:id="@+id/nine_image"
                android:layout_width="0dp"
                android:layout_height="0dp"
                android:src="@drawable/ic_launcher_background"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintHorizontal_bias=".00"
                app:layout_constraintHeight_percent="1"
                app:layout_constraintWidth_percent=".1"
                >
            </ImageView>

            <TextView
                android:id="@+id/nine_text"
                android:layout_width="0dp"
                android:layout_height="0dp"
                android:text="List Order"
                android:textSize="16dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintStart_toEndOf="@id/nine_image"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintHeight_percent=".6"
                app:layout_constraintWidth_percent=".8"
                >
